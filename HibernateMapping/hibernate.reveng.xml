<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="log"></table-filter>
	<table-filter match-name="member"></table-filter>
	<table-filter match-name="member_status"></table-filter>
	<table-filter match-name="member_type"></table-filter>
	<table-filter match-name="message"></table-filter>
	<table-filter match-name="notify"></table-filter>
	<table-filter match-name="order"></table-filter>
	<table-filter match-name="order_product"></table-filter>
	<table-filter match-name="product"></table-filter>
	<table-filter match-name="product_category"></table-filter>
	<table-filter match-name="purchase"></table-filter>
	<table-filter match-name="purchase_product"></table-filter>
	<table-filter match-name="supplier"></table-filter>
</hibernate-reverse-engineering>